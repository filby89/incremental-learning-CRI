{
    "name": "pycharm debug",
    "run_id": "test",
    "n_gpu": 4,

    "loss": "bce_loss",
    "loss_continuous": "mse_loss",
    "metrics": [
        "average_precision", "roc_auc", "f1_score"
    ],
    "metrics_continuous": [
        "r2", "mean_squared_error"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [20, 40],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 60,

        "save_dir": "/gpu-data/filby/EmoReact_V_1.0/experiments_tensorboard/eusipco/",
        "save_period": 1,
        "verbosity": 2,
        
        "monitor": "max val_roc_auc_macro",
        "early_stop": 100,

        "tensorboard": true
    }
}
